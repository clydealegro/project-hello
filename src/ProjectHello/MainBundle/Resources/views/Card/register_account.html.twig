{% extends 'ProjectHelloMainBundle:Default:register_account.html.twig' %}

{% set register_account_action = '' %}

{% block register_email_field %}
	{{ form_widget(form.emailAddress, { attr: { 'class': class_name } }) }}
{% endblock %}

{% block register_password_field %}
    {{ form_widget(form.password, { attr: { 'class': class_name } }) }}
{% endblock %}

{% block register_confirm_password_field %}
    {{ form_widget(form.confirmPassword, { attr: { 'class': class_name } }) }}
{% endblock %}